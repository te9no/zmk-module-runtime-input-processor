#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>
#include <behaviors/runtime-input-processor.dtsi>

&mmv_input_listener {
	input-processors = <&mouse_runtime_input_processor>;
};

&msc_input_listener {
	input-processors = <&scroll_runtime_input_processor>;
};

/ {
	keymap {
		compatible = "zmk,keymap";
		
		default_layer {
			bindings = <
			&kp N0
			&kp N1
			&kp N2
			&kp N3
			&kp N4
			&kp N5
			&kp N6
			&kp N7
			&kp N8
			&kp N9
			&kp ENTER
			>;
		};
	};
};